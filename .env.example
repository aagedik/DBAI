# Private key of deployer (EVM, no 0x prefix or with 0x both accepted by Hardhat)
PRIVATE_KEY=

# BscScan API key for contract verification
BSCSCAN_API_KEY=

# Owner of DBAI (receives initial supply, controls pause/fee-exempt/charity wallet)
OWNER_ADDRESS=0x000000000000000000000000000000000000dEaD

# Charity wallet (receives the 1% fee)
CHARITY_WALLET_ADDRESS=0x000000000000000000000000000000000000bEEF

# For verification
CONTRACT_ADDRESS=

# Vesting deployment parameters (examples provided)
VESTING_TOKEN_ADDRESS=
VESTING_START_TIMESTAMP=1735689600   # 2025-01-01 00:00:00 UTC
VESTING_CLIFF_SECONDS=31536000       # 12 months
VESTING_TOTAL_DURATION_SECONDS=63072000 # 24 months total
BENEFICIARY_ADDRESSES=0x1111111111111111111111111111111111111111,0x2222222222222222222222222222222222222222

# PancakeSwap and launch configuration
# Pancake v2 Router mainnet address (default below)
PANCAKE_ROUTER=0x10ED43C718714eb63d5aA57B78B54704E256024E
# If you already know LP pair address, set it; otherwise leave blank and set later
LP_PAIR_ADDRESS=
# Set to 'true' when you are ready to open transfers for everyone
ENABLE_TRADING=false
# Optional: comma-separated addresses to mark fee-exempt (DEX/router/pair/vesting etc.)
EXEMPT_ADDRESSES=

# Ownership transfer (recommended to a multisig like Gnosis Safe)
NEW_OWNER_ADDRESS=
